<div class="col-6 d-flex justify-content-end">
    <a href="/diet/adddiet/{{user_id}}" type="button" class="btn btn-primary align-self-center">+ add diet</a>
  </div>
<table class="table table-bordered">

  <tbody>

    {{#each rows}}
    <tr>
      <th scope="row">{{this.diet_id}}</th>
      <td>{{this.food_name}}</td>
      <td>{{this.type}}</td>
      <td>{{this.add_date}}</td>
      <td> {{this.user_id}}</td>
      <td class="text-end">
        <a href="/viewuser/{{this.diet_id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i> View</a>
        <a href="/edituser/{{this.diet_id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>
          Edit</a>
        <a href="/{{this.diet_id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i> Delete</a>
      </td>
    </tr>
    {{/each}}
        
  </tbody>
</table>